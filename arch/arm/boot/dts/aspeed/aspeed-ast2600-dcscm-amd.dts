/dts-v1/;

#include "aspeed-g6.dtsi"
#include <dt-bindings/gpio/aspeed-gpio.h>

/ {
	model = "AST2600 DC-SCM AMD";
	compatible = "aspeed,ast2600";

	aliases {
		serial4 = &uart5;
	};

	chosen {
		bootargs = "console=ttyS4,115200n8";
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x80000000 0x80000000>;
	};

	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;

		video_engine_memory: video {
			size = <0x04000000>;
			alignment = <0x01000000>;
			compatible = "shared-dma-pool";
			reusable;
		};

		gfx_memory: framebuffer {
			size = <0x01000000>;
			alignment = <0x01000000>;
			compatible = "shared-dma-pool";
			reusable;
		};

		bmc_dev_memory: bmc_dev_memory {
			size = <0x00100000>;
			alignment = <0x00100000>;
			compatible = "shared-dma-pool";
			no-map;
		};

		ssp_memory: ssp_memory {
			size = <0x02000000>;
			alignment = <0x01000000>;
			shm-size = <0x1000>;
			compatible = "shared-dma-pool";
			no-map;
		};

		mctp0_reserved: mctp0_reserved@b1000000 {
			reg = <0xb1000000 0x10000>;
			compatible = "shared-dma-pool";
			no-map;
		};

		mctp1_reserved: mctp1_reserved@b1010000 {
			reg = <0xb1010000 0x10000>;
			compatible = "shared-dma-pool";
			no-map;
		};
	};
};

&fmc {
	status = "okay";

	flash@0 {
		status = "okay";
		m25p,fast-read;
		label = "bmc";
		spi-max-frequency = <12500000>;
		spi-tx-bus-width = <4>;
		spi-rx-bus-width = <4>;
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			u-boot@0 {
				reg = <0x0 0x140000>; // 1280KB
				label = "u-boot";
			};

			u-boot-env@140000 {
				reg = <0x140000 0x20000>; // 128KB
				label = "u-boot-env";
			};

			kernel@160000 {
				reg = <0x160000 0x800000>; // 8MB
				label = "kernel";
			};

			rofs@960000 {
				reg = <0x960000 0x24a0000>; // 36.625MB
				label = "rofs";
			};

			rwfs@2e00000 {
				reg = <0x2e00000 0xfe0000>; // 16256KB
				label = "rwfs";
			};

			pfm@3de0000 {
				reg = <0x3de0000 0x20000>; // 128KB
				label = "pfm";
			};
			reserved-1@3e00000 {
				reg = <0x3e00000 0x200000>; // 2048KB
				label = "reserved-1";
			};

			rc-image@4000000 {
				reg = <0x4000000 0x4000000>; // 65536KB
				label = "rc-image";
			};

			image-stg@8000000 {
				reg = <0x8000000 0x4000000>; // 65536KB
				label = "img-stg";
			};

			pfr-stg@c000000 {
				reg = <0xc000000 0x100000>; // 1024KB
				label = "pfr-stg";
			};

			cpld-stg@c100000 {
				reg = <0xc100000 0x400000>; // 4096KB
				label = "cpld-stg";
			};

			reserved-2@c500000 {
				reg = <0xc500000 0x3b00000>; // 60416KB
				label = "reserved-2";
			};
		};
	};

	flash@1 {
		status = "disabled";
		m25p,fast-read;
		label = "fmc0:1";
		spi-max-frequency = <12500000>;
		spi-tx-bus-width = <1>;
		spi-rx-bus-width = <1>;
	};

	flash@2 {
		status = "disabled";
		m25p,fast-read;
		label = "fmc0:2";
		spi-max-frequency = <12500000>;
		spi-tx-bus-width = <1>;
		spi-rx-bus-width = <1>;
	};
};

&spi1 {
	pinctrl-0 = <&pinctrl_spi1_default &pinctrl_spi1cs1_default
				&pinctrl_qspi1_default>;
	status = "okay";

	flash@0 {
		status = "okay";
		m25p,fast-read;
		label = "spi1:0";
		spi-max-frequency = <12500000>;
		spi-tx-bus-width = <1>;
		spi-rx-bus-width = <1>;
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			spi1_pch_pfm@0 {
				reg = <0x0 0x10000>;
				label = "spi1_pch_pfm";
			};

			spi1_pch_stg@10000 {
				reg = <0x10000 0xf00000>;
				label = "spi1_pch_stg";
			};

			spi1_pch_reserved1@f10000 {
				reg = <0xf10000 0x650000>;
				label = "spi1_pch_reserved1";
			};

			// pch_gpnv 0x1560000 0x14000
			// pch_reserved2 0x1574000 0xc000
			spi1_pch_gpnv_reserved2@1560000 {
				reg = <0x1560000 0x20000>;
				label = "spi1_pch_gpnv_reserved2";
			};

			// pch_fv_whea 0x1580000 0x4000
			// pch_fv_main 0x1584000 0x77c000
			spi1_pch_fv_whea_main@1580000 {
				reg = <0x1580000 0x780000>;
				label = "spi1_pch_fv_whea_main";
			};

			spi1_pch_fv_bb@1d00000 {
				reg = <0x1d00000 0x300000>;
				label = "spi1_pch_fv_bb";
			};

			spi1_pch_rc@2000000 {
				reg = <0x2000000 0x2000000>;
				label = "spi1_pch_rc";
			};
		};
	};

	flash@1 {
		status = "okay";
		m25p,fast-read;
		label = "spi1:1";
		spi-max-frequency = <12500000>;
		spi-tx-bus-width = <1>;
		spi-rx-bus-width = <1>;
	};
};

&spi2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_spi2_default &pinctrl_spi2cs1_default
				&pinctrl_spi2cs2_default &pinctrl_qspi2_default>;
	status = "disabled";

	flash@0 {
		status = "disabled";
		m25p,fast-read;
		label = "spi2:0";
		spi-max-frequency = <12500000>;
		spi-tx-bus-width = <1>;
		spi-rx-bus-width = <1>;
	};

	flash@1 {
		status = "disabled";
		m25p,fast-read;
		label = "spi2:1";
		spi-max-frequency = <12500000>;
		spi-tx-bus-width = <1>;
		spi-rx-bus-width = <1>;
	};

	flash@2 {
		status = "disabled";
		m25p,fast-read;
		label = "spi2:2";
		spi-max-frequency = <12500000>;
		spi-tx-bus-width = <1>;
		spi-rx-bus-width = <1>;
	};

};

