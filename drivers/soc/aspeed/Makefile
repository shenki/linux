obj-$(CONFIG_AST_PECI)		+= ast_peci.o
obj-$(CONFIG_AST_MBX)		+= ast_mailbox.o
obj-$(CONFIG_AST_JPEG)		+= ast-jpeg.o
obj-$(CONFIG_AST_JTAG)		+= ast_jtag.o
obj-$(CONFIG_AST_HID)		+= ast_hid.o
